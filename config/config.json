{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "employee-directory-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeDirectory/EmployeeDirectoryWebPart.js",
          "manifest": "./src/webparts/employeeDirectory/EmployeeDirectoryWebPart.manifest.json"
        }
      ]
    },
    "mcc-service-request-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mccServiceRequest/MccServiceRequestWebPart.js",
          "manifest": "./src/webparts/mccServiceRequest/MccServiceRequestWebPart.manifest.json"
        }
      ]
    },
    "mcc-service-actions-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mccServiceActions/MccServiceActionsWebPart.js",
          "manifest": "./src/webparts/mccServiceActions/MccServiceActionsWebPart.manifest.json"
        }
      ]
    },
    "mcc-requester-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mccRequesterView/MccRequesterViewWebPart.js",
          "manifest": "./src/webparts/mccRequesterView/MccRequesterViewWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.5.1.min.js",
      "globalName": "jQuery"
    },
    "devextreme": {
      "path": "https://cdn3.devexpress.com/jslib/23.2.4/js/dx.all.js",
      "globalName": "devextreme",
      "globalDependencies": [
        "jquery"
      ]
    },
    "@pnp/pnpjs": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/sp-pnp-js/3.0.10/pnp.min.js",
      "globalName": "pnp.sp"
    }
  },
  "localizedResources": {
    "EmployeeDirectoryWebPartStrings": "lib/webparts/employeeDirectory/loc/{locale}.js",
    "MccServiceRequestWebPartStrings": "lib/webparts/mccServiceRequest/loc/{locale}.js",
    "MccServiceActionsWebPartStrings": "lib/webparts/mccServiceActions/loc/{locale}.js",
    "MccRequesterViewWebPartStrings": "lib/webparts/mccRequesterView/loc/{locale}.js"
  }
}
